{
    "comments": {
        "blockComment": [
            "===",
            "==="
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string",
                "comment"
            ]
        }
    ],
    "folding": {
        "markers": {
            "start": "\\s*(run)\\s+",
            "end": "\\s*(end)\\s+"
        }
    },
    "wordPattern": "\\\\b\\\\w+\\\\b"
}